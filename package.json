{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "NODE_OPTIONS='--no-deprecation' vite",
    "build": "tsc && vite build",
    "build:electron": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.electron.json",
    "build:renderer": "vite build",
    "preview": "vite preview",
    "start": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on tcp:5173 && cross-env NODE_ENV=development NODE_OPTIONS='--no-deprecation' electron .\"",
    "prepare:web": "node prepare-web-build.js",
    "prepare:netlify": "node --experimental-modules prepare-netlify-env.js",
    "build:web": "npm run prepare:web && cross-env VITE_IS_WEB_BUILD=true tsc && cross-env VITE_IS_WEB_BUILD=true vite build",
    "restore:backups": "node restore-backups.js",
    "dev:web": "cross-env VITE_IS_WEB_BUILD=true vite",
    "package:electron": "npm run build:electron && electron-builder",
    "netlify:build": "npm run build:web",
    "netlify:preview": "npm run build:web && netlify deploy --dir=dist/renderer",
    "netlify:deploy": "npm run build:web && netlify deploy --prod --dir=dist/renderer",
    "dev:full": "concurrently \"npm run dev:web\" \"npm run netlify:dev\"",
    "test": "npm run test:backend && npm run test:frontend",
    "test:frontend": "vitest run",
    "test:backend": "jest",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:functions": "node test-feedback.mjs",
    "netlify:dev": "netlify dev"
  },
